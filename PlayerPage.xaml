<Page x:Class="GuidingStarKursach.PlayerPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:GuidingStarKursach"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PlayerPage">

    <Grid Background="White">
        <Button x:Name="Back" Content="Back" HorizontalAlignment="Left" Height="75" Margin="10,10,0,0" VerticalAlignment="Top" Width="143" Click="Back_Click"/>
        <Button x:Name="Attacks" Content="Attacks" HorizontalAlignment="Left" Height="89" Margin="10,111,0,0" VerticalAlignment="Top" Width="143" Click="Button_Click"/>
        <Button x:Name="Inventory" Content="Inventory" HorizontalAlignment="Left" Height="97" Margin="10,225,0,0" VerticalAlignment="Top" Width="143" Click="Inventory_Click"/>

    </Grid>
</Page>
